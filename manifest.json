{
    "name" : "Ugly Japanese Font Fix",
    "description": "This extension just fixed Ugly Japanese Font rendering.",
    "version" : "1.0.0.0",
    "author" : "drikin",
    "manifest_version": 2,

    "icons" : {
        "16" : "icons/icon_16.png",
        "48" : "icons/icon_48.png",
        "128" : "icons/icon_128.png"
    },

    "content_scripts" : [{
            "css": ["ugly-jp-font-face.css"],
            "matches" : ["*://*/*"]
        }
    ]
}